<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Circle">
        <attr name="circleBackground" format="integer" />
        <attr name="firstChildPosition">
            <enum name="East" value="0" />
            <enum name="South" value="90" />
            <enum name="West" value="180" />
            <enum name="North" value="270" />
        </attr>
        <attr name="rotateToCenter" format="boolean" />
        <attr name="isRotating" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="maxProgress" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="startAngle" format="integer" />
        <attr name="sweepAngle" format="integer" />
        <attr name="progressStrokeWidth" format="dimension" />
        <attr name="progressBGStrokeWidth" format="dimension" />
        <attr name="CircleBgColor" format="color" />
        <attr name="CirclePaintColor" format="color" />
        <attr name="CircleProgressBgColor" format="color" />
        <attr name="CircleInsideBgColor" format="color" />
        <attr name="drawCircleInsideBG" format="boolean"/>
        <attr name="circleMargin" format="dimension"/>
    </declare-styleable>

    <dimen name="charging_battery_head_size">56dp</dimen>
    <dimen name="charging_battery_head_title_padding_left">16dp</dimen>
    <dimen name="charging_battery_head_title_text_size">32dp</dimen>
    <dimen name="charging_battery_head_ampm_size">30dp</dimen>
    <dimen name="charging_battery_head_ampm_margins">4dp</dimen>
    <dimen name="charging_battery_head_ampm_text">12dp</dimen>
    <dimen name="charging_battery_head_margin">8dp</dimen>
    <dimen name="charging_battery_48">48dp</dimen>
    <dimen name="charging_battery_24">24dp</dimen>
    <dimen name="charging_battery_18">18dp</dimen>
    <dimen name="charging_battery_14">14dp</dimen>
    <dimen name="charging_battery_2">2dp</dimen>
    <dimen name="charging_battery_6">6dp</dimen>
    <dimen name="charging_battery_20">20dp</dimen>
    <dimen name="charging_battery_minus">-2dp</dimen>
    <dimen name="charging_battery_10">10dp</dimen>
    <dimen name="charging_battery_148">148dp</dimen>
    <dimen name="charging_battery_152">152dp</dimen>
    <dimen name="charging_battery_60">60dp</dimen>
    <dimen name="charging_battery_22">22dp</dimen>
    <dimen name="charging_battery_128">128dp</dimen>
    <dimen name="charging_battery_main_text_size">14dp</dimen>

    <declare-styleable name="ShimmerFrameLayout">
        <attr name="auto_start" format="boolean"/>
        <attr name="base_alpha" format="float"/>
        <attr name="duration" format="integer"/>
        <attr name="repeat_count" format="integer"/>
        <attr name="repeat_delay" format="integer"/>
        <attr name="repeat_mode" format="enum">
            <enum name="restart" value="1"/>
            <enum name="reverse" value="2"/>
        </attr>
        <attr name="angle" format="enum">
            <enum name="cw_0" value="0"/>
            <enum name="cw_90" value="90"/>
            <enum name="cw_180" value="180"/>
            <enum name="cw_270" value="270"/>
        </attr>
        <attr name="dropoff" format="float"/>
        <attr name="fixed_width" format="dimension"/>
        <attr name="fixed_height" format="dimension"/>
        <attr name="intensity" format="float"/>
        <attr name="relative_width" format="float"/>
        <attr name="relative_height" format="float"/>
        <attr name="shape" format="enum">
            <enum name="linear" value="0"/>
            <enum name="radial" value="1"/>
        </attr>
        <attr name="tilt" format="float"/>
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <!-- the radius size of the round rect -->
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="waveViewStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="above_wave_color" format="color"/>
        <attr name="blow_wave_color" format="color"/>
        <attr name="wave_progress" format="integer"/>
        <attr name="below_wave_height_factor" format="float"/>
        <attr name="wave_length" format="enum">
            <enum name="large" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="little" value="3"/>
        </attr>
        <attr name="wave_height" format="enum">
            <enum name="large" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="little" value="3"/>
        </attr>
        <attr name="wave_hz" format="enum">
            <enum name="fast" value="1"/>
            <enum name="normal" value="2"/>
            <enum name="slow" value="3"/>
        </attr>
    </declare-styleable>
    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

</resources>  