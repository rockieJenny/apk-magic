<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" 
    android:shape="rectangle">
            <!-- 填充颜色 -->
    <solid android:color="#00000000"></solid>

    <corners android:radius="20.0dp" />
    <!-- 圆角，你也可以对不同的角设置不同的数值 -->
    <!-- 线的宽度，颜色灰色 -->
